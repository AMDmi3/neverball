
all :
	(cd libvec;  $(MAKE))
	(cd libsol;  $(MAKE))
	(cd libglw;  $(MAKE))
	(cd libglv;  $(MAKE))
	(cd putt;    $(MAKE))
#	(cd ball;    $(MAKE))
	(cd map2sol; $(MAKE))

install :
	(cd libvec;  $(MAKE) install)
	(cd libsol;  $(MAKE) install)
	(cd libglw;  $(MAKE) install)
	(cd libglv;  $(MAKE) install)
	(cd putt;    $(MAKE) install)
#	(cd ball;    $(MAKE) install)
	(cd map2sol; $(MAKE) install)

clean :
	(cd libvec;  $(MAKE) clean)
	(cd libsol;  $(MAKE) clean)
	(cd libglw;  $(MAKE) clean)
	(cd libglv;  $(MAKE) clean)
	(cd putt;    $(MAKE) clean)
#	(cd ball;    $(MAKE) clean)
	(cd map2sol; $(MAKE) clean)

